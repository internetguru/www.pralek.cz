<?xml version="1.0" encoding="utf-8"?>
<LinkList>
  <var id="template">long</var>
  <item id="long" modifyonly="modifyonly">
    <dl class="clanek card card--shadow card--image col--3"  var="doclist-new@class link@class">
      <dt><a title="Zobrazit článek %s" var="link@href short@title" href="%s?usp=image"><img var="image@src heading@alt"/></a></dt>
      <dd class="date" fn="inputvar-dateformat" var="ctime">n/a</dd>
      <dd class="kw" var="kw" fn="inputvar-maketags">n/a</dd>
      <dt class="heading card__heading"><a title="Zobrazit článek %s" var="link@href short@title heading" href="%s?usp=heading">n/a</a></dt>
      <dd class="desc" var="desc">n/a</dd>
      <dd class="extra"><ul class="button-list">
        <li>
          <a class="button button--simple button--simple" title="Zobrazit článek %s" var="link@href short@title" href="%s?usp=button">Zobrazit článek</a></li>
      </ul></dd>
      <dd class="print"><span var="heading">n/a</span>, /<span var="link">n/a</span></dd>
    </dl>
  </item>
</LinkList>
